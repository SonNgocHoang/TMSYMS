<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fr_Calendar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="tms.com.libre.tms.AcMain">

    <include layout="@layout/toolbar" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:orientation="horizontal">

        <Button
            android:id="@+id/gotoPickUp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Go to Pick up" />

        <Button
            android:id="@+id/gotoOnRoute"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Go to On Route" />
    </LinearLayout>

    <GridView
        android:id="@+id/gridviewTruckLoad"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:numColumns="1"></GridView>


</LinearLayout>
